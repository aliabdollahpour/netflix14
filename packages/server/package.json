{
    "name": "server",
    "scripts": {
        "start:dev": "node index",
        "start:demo": "DISABLE_SYNC=true NODE_ENV=production node index",
        "start:prod": "NODE_ENV=production node index",
        "db:migrate": "rm -rf prisma/migrations; npx prisma migrate dev",
        "db:browse": "npx prisma studio",
        "db:clear": "npx prisma migrate reset",
        "db:clear-not-available": "DISABLE_SYNC=true node -e 'require(\"./jobs\").clearNotAvailable()'",
        "start": "npm run start:prod --prefix ../client; npm run start:demo"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "dependencies": {
        "@prisma/client": "^4.6.0",
        "axios": "^1.1.3",
        "body-parser": "^1.20.1",
        "chokidar": "^3.5.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "figlet": "^1.5.2",
        "qs": "^6.11.0",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "dotenv": "^16.0.3",
        "prisma": "^4.6.0"
    }
}
